{
  "name": "@archwayhq/arch3-proto",
  "version": "0.1.0",
  "description": "Archway Protobuf client",
  "homepage": "https://github.com/jsoneaday/arch3-proto#readme",
  "repository": "https://github.com/jsoneaday/arch3-proto",
  "keywords": [
    "archway",
    "cosmos",
    "cosmjs",
    "cosmosjs",
    "cosmwasm",
    "tendermint",
    "atom",
    "wasm",
    "smart-contracts",
    "blockchain",
    "dapp",
    "dapps",
    "nft",
    "tooling",
    "web3"
  ],
  "author": "Archway Network (https://archway.io)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/archway-network/arch3.js/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "build",
    "./*.md",
    "./LICENSE"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "prepack": "yarn run build",
    "clean": "rimraf ./build",
    "codegen": "./scripts/codegen.js",
    "build": "yarn clean && tsc --build",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.cjs,.ts",
    "lint:fix": "yarn lint --fix",
    "lint:sarif": "yarn lint --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
    "docs": "typedoc --options typedoc.cjs ."
  },
  "dependencies": {
    "@cosmjs/amino": "^0.30.1",
    "@cosmjs/proto-signing": "^0.30.1",
    "@cosmjs/stargate": "^0.30.1",
    "@cosmjs/tendermint-rpc": "^0.30.1",
    "protobufjs": "^6.11.3"
  },
  "devDependencies": {
    "@osmonauts/telescope": "^0.94.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.13",
    "@types/webgl2": "^0.0.7",
    "eslint": "8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "long": "^5.2.0",
    "prettier": "^2.8.7",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}

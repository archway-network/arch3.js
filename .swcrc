{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "commonjs"
  },
  "jsc": {
    "baseUrl": ".",
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true,
      "decorators": true
    },
    "target": "es2018",
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": true
  },
  "minify": false
}
